<Window
    x:Class="TnyavnToSpotifySongDownloader.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:hc="https://handyorg.github.io/handycontrol"
    xmlns:local="clr-namespace:TnyavnToSpotifySongDownloader"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Title="TnyavnTo Spotify Playlist Downloader"
    Height="650"
    Width="1000"
    WindowStartupLocation="CenterScreen"
    mc:Ignorable="d">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="66*"/>
            <ColumnDefinition Width="59*"/>
        </Grid.ColumnDefinitions>
        <Frame x:Name="NavigationFrame" Grid.ColumnSpan="2" Navigated="NavigationFrame_Navigated"/>
    </Grid>
</Window>
